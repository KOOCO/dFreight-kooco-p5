                                            <a class='dropdown-item' href='#'>
                                                <i class="fa fa-lock"></i> @L["Btn:Lock"]
                                            </a>
                                            <p class="dropdown-divider"></p>
                                            <a class='dropdown-item' href='#'>
                                                <i class="fa fa-file-text-o"></i> @L["Btn:Arrival Notice"]
