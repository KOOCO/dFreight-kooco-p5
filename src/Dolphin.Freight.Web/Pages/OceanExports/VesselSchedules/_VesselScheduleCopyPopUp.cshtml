@model Dolphin.Freight.Web.ViewModels.VesselSchedules.VesselSchedulePopUpViewModel;<div class="modal-dialog" ng-class="size ? 'modal-' + size : ''">    <div class="modal-content" uib-modal-transclude="">        <!-- Modal DocumentPackage -->        <div class="modal-header ng-scope">            <h4 class="modal-title ng-binding">Copy Vessel Schedule</h4>            <button aria-hidden="true" ng-click="cancel()" class="close" type="button"></button>        </div>        <div class="modal-body ng-scope" bind-all-uniform="">            <!--                    <h4 class="modal-title" ng-bind-html="msg"></h4>                -->            <div class="row">                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">                    <div class="form-body-tool">                        Please select how would you like to copy Vessel Schedule information ?                    </div>                    <br>                    <div class="form-group form-sm ng-scope">                        <label class="checkbox-inline ng-binding ng-scope" style="font-weight:700;">                            <div class="checker" style="display:inline;"><span><input type="checkbox" id="HBLPrintCB"></span></div>                            Copy vessel info and all shipping schedule                        </label>                    </div>                </div>            </div>            @if (Model.IsBookingExists)
            {                <div class="row">                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">                        <div class="form-body-tool">                            Please select how would you like to copy Booking under this Vessel Schedule ?                        </div>                        <br>                        <div class="form-group form-sm ng-scope" ng-repeat="report_list in report_group">                            <label class="radio-inline" style="font-weight:700;">                                <div class="radio" style="display:inline;"><span class="checked"><input type="radio" name="report-type" value="vesselOnly" checked></span></div>                                    <font style="vertical-align: inherit;">                                        Copy this vessel schedule only                                    </font>                            </label>                            <label class="radio-inline" style="font-weight:700;">                                <div class="radio" style="display:inline;"><span><input type="radio" name="report-type" value="vesselBookings"></span></div>                                    <font style="vertical-align: inherit;">                                        Copy all booking and this vessel shchedule                                    </font>                            </label>                        </div>                    </div>                </div>            }        </div>        <div class="modal-footer ng-scope">            <button type="button" id="docPkgCancleBtn" onclick="Cancel()" class="btn default">Cancel</button>            <button class="btn blue btn-primary" id="HBLDocPKGBtn" onclick="onSave()" type="button" focus-if="any_selected()" disabled="disabled">                Ok            </button>        </div>    </div></div>