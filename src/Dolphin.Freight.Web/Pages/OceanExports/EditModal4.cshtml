@page
@using Dolphin.Freight.Localization
@using Dolphin.Freight.Web.Pages.Components
@using Microsoft.Extensions.Localization
@model Dolphin.Freight.Web.Pages.OceanExports.EditModal4Model
@inject IStringLocalizer<FreightResource> L
@section styles
{
     <style type="text/css">
.files input {
    outline: 2px dashed #92b0b3;
    outline-offset: -10px;
    -webkit-transition: outline-offset .15s ease-in-out, background-color .15s linear;
    transition: outline-offset .15s ease-in-out, background-color .15s linear;
    padding: 100px 0px 85px 10%;
    text-align: center !important;
    margin: 0;
    width: 100% !important;
}
.files input:focus{     outline: 2px dashed #92b0b3;  outline-offset: -10px;
    -webkit-transition: outline-offset .15s ease-in-out, background-color .15s linear;
    transition: outline-offset .15s ease-in-out, background-color .15s linear; border:1px solid #92b0b3;
 }
.files{ position:relative}
.files:after {  pointer-events: none;
    position: absolute;
    top: 60px;
    left: 0;
    width: 50px;
    right: 0;
    height: 56px;
    content: "";
    /* background-image: url(https://image.flaticon.com/icons/png/128/109/109612.png); */
    display: block;
    margin: 0 auto;
    background-size: 100%;
    background-repeat: no-repeat;
}
.color input{ background-color:#f1f1f1;}
.files:before {
    position: absolute;
    bottom: 10px;
    left: 0;  pointer-events: none;
    width: 100%;
    right: 0;
    height: 57px;
    content: " or drag it here. ";
    display: block;
    margin: 0 auto;
    color: #2ea591;
    font-weight: 600;
    text-transform: capitalize;
    text-align: center;
}
 </style>
}
@section scripts
    {
    @*<abp-script src="/Pages/OceanExports/CreateMbl.js" />*@
    <script>
        Swal.fire({
                        title: 'Loading...',
                        didOpen: () => {
                            Swal.showLoading()
                        }
                    })
        function changeHid(id, hid) {
            location.href = '/OceanExports/EditModal4?Id=' + id + "&Hid=" + hid
        }
        var l = abp.localization.getResource('Freight');
        var isLocked;
        $(document).ready(function () {
            isLocked = '@Model.OceanExportMbl.IsLocked';
            if (isLocked == 'True') abp.message.warn(l("Message:LockMessage"));
            var IsShowHbl = '@Model.IsShowHbl';
            if (IsShowHbl == 'True') $("#hblCard").show();
            else $("#hblCard").hide();
            $("#addHBtn").click(function () {
                var url = "/OceanExports/EditModal?Id=@Model.Id&NewHbl=1";
                location.href = url;
            })
            initAttments(0, '@Model.Id');
            initAttments(1, '@Model.Hid');
            $("#uploadFile").change(function () {
                var fid = '@Model.Id';
                var files = $('#uploadFile').prop("files");
                var url = "EditModal4?handler=MyUploader";
                formData = new FormData();
                         
                          for (var i = 0; i < files.length; i++) {
        formData.append("MyUploader", files[i]);
    }
                formData.append("fid", fid);
                formData.append("ftype", 2);
                jQuery.ajax({
                    type: 'POST',
                    url: url,
                    data: formData,
                    cache: false,
                    contentType: false,
                    processData: false,
                    beforeSend: function (xhr) {
                        xhr.setRequestHeader("XSRF-TOKEN",
                            $('input:hidden[name="__RequestVerificationToken"]').val());
                    },
                    success: function (repo) {

                        initAttments(0, fid);
                    },
                    error: function (rs) {
                        console.log(rs)
                        alert("Error occurs");
                    }
                });
            });
            $("#uploadHFile").change(function () {
                debugger;
                var fid = '@Model.Hid';
                var files = $('#uploadHFile').prop("files");
                var url = "EditModal4?handler=MyUploader";
                formData = new FormData();
               for (var i = 0; i < files.length; i++) {
        formData.append("MyUploader", files[i]);
    }
                formData.append("fid", fid);
                formData.append("ftype", 2);
                jQuery.ajax({
                    type: 'POST',
                    url: url,
                    data: formData,
                    cache: false,
                    contentType: false,
                    processData: false,
                    beforeSend: function (xhr) {
                        xhr.setRequestHeader("XSRF-TOKEN",
                            $('input:hidden[name="__RequestVerificationToken"]').val());
                    },
                    success: function (repo) {
                        initAttments(1, fid);
                    },
                    error: function (rs) {
                        console.log(rs)
                        alert("Error occurs");
                    }
                });
            });

            var url = new URL(window.location.href);

            if(!url.searchParams.get('Hid')){
                $('.borad-cont').each(function(i, e)
                {
                    if($(e).parent().hasClass('focus')){
                        $(e).click();
                    }
                });
            }
            if (isLocked != 'True') {
                $('#lockButtonMbl').empty().html('<i class="fa fa-lock"></i> ' + l("Btn:Lock"));
                $('#ActionDropMbl').empty().html('<i class="fa fa-lock-open"></i> ' + l("Display:Function"));
            }
            else {
                $('#ActionDropMbl').empty().html('<i class="fa fa-lock"></i> ' + l("Display:Function"));
                $('#lockButtonMbl').empty().html('<i class="fa fa-lock-open"></i> ' + l("Btn:UnLock"));
            }
            var isLockedHbl = '@Model.OceanExportHbl.IsLocked';
            if (isLockedHbl != 'True') {
                $('#ActionDropHbl').empty().html('<i class="fa fa-lock-open"></i> ' + l("Display:Function"));
                $('#lockButtonHbl').empty().html('<i class="fa fa-lock"></i> ' + l("Btn:Lock"));
            }
            else {
                $('#ActionDropHbl').empty().html('<i class="fa fa-lock"></i> ' + l("Display:Function"));
                $('#lockButtonHbl').empty().html('<i class="fa fa-lock-open"></i> ' + l("Btn:UnLock"));
            }
            Swal.clickCancel();
        });

        function lockUnlockMbl(id) {
            var isLock = $('#lockButtonMbl').find('i').hasClass('fa-lock');
            abp.message.confirm(l(isLock ? 'UnlockConfirmationMessage' : 'LockConfirmationMessage')).then(function (confirmed) {
                if (confirmed) {
                    Swal.fire({
                        title: 'Loading...',
                        didOpen: () => {
                            Swal.showLoading()
                        }
                    })
                    dolphin.freight.importExport.oceanExports.oceanExportMbl.lockedOrUnLockedOceanExportMbl({ MbId: id })
                        .done(function () {
                            Swal.clickCancel();
                            if (!isLock) {
                                $('#lockButtonMbl').empty().html('<i class="fa fa-lock-open"></i> ' + l("Btn:UnLock"));
                                $('#ActionDropMbl').empty().html('<i class="fa fa-lock"></i> ' + l("Display:Function"));
                                abp.message.success(l('Message:SuccessLock'));
                            } else {
                                $('#ActionDropMbl').empty().html('<i class="fa fa-lock-open"></i> ' + l("Display:Function"));
                                $('#lockButtonMbl').empty().html('<i class="fa fa-lock"></i> ' + l("Btn:Lock"));
                                abp.message.success(l('Message:SuccessUnlock'));
                            }
                            
                        });
                }
            });
        }

        function lockUnlockHbl(id) {
            var isLock = $('#lockButtonHbl').find('i').hasClass('fa-lock');
            abp.message.confirm(l(isLock ? 'UnlockConfirmationMessage' : 'LockConfirmationMessage')).then(function (confirmed) {
                if (confirmed) {
                    Swal.fire({
                        title: 'Loading...',
                        didOpen: () => {
                            Swal.showLoading()
                        }
                    })
                    dolphin.freight.importExport.oceanExports.oceanExportHbl.lockedOrUnLockedOceanExportHbl({ HblId: id })
                        .done(function () {
                            Swal.clickCancel();
                            if (!isLock) {
                                $('#ActionDropHbl').empty().html('<i class="fa fa-lock"></i>' + l("Display:Function"));
                                $('#lockButtonHbl').empty().html('<i class="fa fa-lock-open"></i>' + l("Btn:UnLock"));
                            abp.message.success(l('Message:SuccessLock'));
                            } else {
                                $('#ActionDropHbl').empty().html('<i class="fa fa-lock-open"></i>' + l("Display:Function"));
                                $('#lockButtonHbl').empty().html('<i class="fa fa-lock"></i>' + l("Btn:Lock"));
                            abp.message.success(l('Message:SuccessUnlock'));
                            }
                            
                            
                        });
                }
            });
        }
        function initAttments(tindex, qid) {
            dolphin.freight.importExport.attachments.attachment.queryList({ queryType: 2, queryId: qid }).done(function (result) {
                if (result != null && result.length > 0) {
                    var l = abp.localization.getResource('Freight');
                    var trhtml = "";
                    for (var index = 0; index < result.length; index++) {
                        if (tindex == 0) trhtml = trhtml + "<tr id='tr_";
                        else trhtml = trhtml + "<tr id='trh_";
                        trhtml = trhtml + index + "'><td>" + result[index].fileName + "</td><td>" + result[index].creationTime.substring(0, 10) + "</td> <td>" + result[index].size + "KB </td><td>";
                        trhtml=trhtml+"<select disabled><option >Other<option></select> </td><td>";
                        // trhtml = trhtml + "<div class='btn-group dropdown'><button class='btn btn-secondary dropdown-toggle'  type='button' id='ActionDrop_" + index + "' data-bs-toggle='dropdown'>";
                        // trhtml = trhtml + l('Display:Function');
                        // trhtml = trhtml + "</button><div class='dropdown-menu ' aria-labelledby='dropdownMenuButton1' id='ActionDrop_div_" + index + "'>";
                        trhtml = trhtml + "<a   href = 'EditModal4?handler=Download&filename=" + result[index].fileName + "'  ><li style='padding: 3px;' class='fa fa-cloud-download' data - toggle='tooltip' data - placement='top' title = 'Download File' > </li>";
                        // trhtml = trhtml + l('Btn:Download');
                        trhtml = trhtml + "</a><a  href='javascript:void(0)' data-toggle='tooltip' data-placement='top' title='Delete File' onclick='deleteFile(" + index + ",\"" + result[index].id + "\"," + tindex + ")' ><i style='padding: 3px;' class='fa fa-trash'></i>"
                        // trhtml = trhtml + l('Btn:Delete');"
                        trhtml = trhtml + "</a><a href='javascript:void(0)' onclick='openImagePreview(\"" + result[index].fileName + "\")'><i style='padding: 3px;' class='fa fa-eye' data-toggle='tooltip' data-placement='top' title='Preview Image'></i> </a></td><td>" + result[index].creatorName;
                         trhtml = trhtml + "</td></tr >" ;
                    }
                    if (tindex == 0) {
                        $("#ftbody").html(trhtml);
                        $("#uploadFile").val("");
                    } else {
                        $("#htbody").html(trhtml);
                        $("#uploadHFile").val("");
                    }

                }
            });
        }

        function deleteFile(index, fid, tindex) {

            dolphin.freight.importExport.attachments.attachment.delete(fid)
                .then(function () {
                    
                    abp.message.success(l('SuccessfullyDeleted'));
                    if (tindex == 0) {
                        $("#tr_" + index).remove();
                    }
                    else {
                        $("#trh_" + index).remove();
                    }

                });
        }
        function openImagePreview(imageUrl) {


            window.open("/mediaUpload/" + imageUrl, '_blank');


          
   
}

        function getReportTelexrelease() {
                   var idNo = $('#OceanExportMbl_Id').val();
            myWindow = window.open("@Url.Action("Telexrelease", "Docs")" + "?id=" + idNo, 'Telexrelease', 'width=1200,height=800');
            myWindow.focus()
        }
        function gotoReport(rindex) {
            var url = "";
            var pname = "";
            switch (rindex) {
                default:
                    url = '@Url.Action("Car", "Docs")';
                    pname = "Car";
                    break;
                case 2:
                    url = '@Url.Action("Cargo", "Docs")';
                    pname = "Cargo";
                    break;
                case 3:
                    url = '@Url.Action("Box", "Docs")';
                    pname = "Box";
                    break;
                case 4:
                    url = '@Url.Action("ProfitSummary", "Docs")';
                    pname = "ProfitSummary";
                    break;
                case 5:
                    url = '@Url.Action("ProfitDetail", "Docs")';
                    pname = "ProfitDetail";
                    break;
                case 6:
                    var idNo = $('#OceanExportHbl_Id').val();
                    url = '@Url.Action("Shipment", "Docs")' + "?id=" + idNo;
                    pname = "Shipment";
                    break;
                case 7:
                    var idNo = $('#OceanExportHbl_Id').val();
                    url = '@Url.Action("OBL", "Docs")' + "?id=" + idNo;
                    pname = "OBL";
                    break;
            }
            myWindow = window.open(url, pname, 'width=1200,height=800')
            myWindow.focus()
        }
