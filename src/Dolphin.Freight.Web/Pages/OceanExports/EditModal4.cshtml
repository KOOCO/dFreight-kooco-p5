            myWindow.focus()
        }
        function getDockRecepit() {
            var idNo = $('#OceanExportHbl_Id').val();
            myWindow = window.open("@Url.Action("DockRecepit", "Docs")" + "?id=" + idNo, 'DockRecepit', 'width=1200,height=800');
