                                                        <input asp-for="MawbModel.RouteTrans3DepatureDate" abp-data-datepicker="false" size="Small" />
                                                    </td>
                                                    <td>
                                                        <input asp-for="MawbModel.RouteTrans3FlightNo" size="Small" />
                                                    </td>
