                                    <td id="totalKG">0.00</td>
                                    <td id="totalLB">0.00</td>
                                    <td id="totalCBM">0.00</td>
                                    <td id="totalCFT">0.00</td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    <div class="modal-footer">
