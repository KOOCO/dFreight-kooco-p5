@using Dolphin.Freight.Localization;
@using Microsoft.Extensions.Localization;
@using Dolphin.Freight.Web.Pages.Components
@using Dolphin.Freight.Web.CommonService;
@model Dolphin.Freight.ImportExport.AirImports.AirImportDetails
@inject IStringLocalizer<FreightResource> L
@inject IDropdownService _dropdown
<style>
    .sortable-list__item {
        padding: 5px 22px 5px 7px;
        color: #fff;
        margin-bottom: 5px;
        background: #AAAAAA url(data:image/svg+xml;base64,) calc(100% - 8px) 10px no-repeat;
    }
</style>
<script>
    function updateMawbParent() {
        debugger;
        var mawbId = $("#Id").val();
        $.ajax({
            url: abp.appPath + 'Docs/AirImportChangeMawbParent?mawbId=' + mawbId'&prevMawbId=@Model.MawbId&hawbId=@Model.HawbId',
            type: 'post',
            dataType: "html",
            success: function (res) {
                $('.modal-content').html(res);
                $('.modal').modal('show');
            }
        });
    }
</script>
<form id="frmManifest" data-check-form-on-close="false">
    <abp-modal>
        <h5 class="modal-title" style="color: black; padding: 1rem;">Mawb List</h5>
        <abp-modal-body class="overflow-auto" style="height:400px;">
            <div class="row">
                <div class="col-xs-12">
                    <div class="form-group form-full">
                        @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "Id", AspItems = _dropdown.MawbLookupList, IsShowLabel = true, FieldName = "", SelectType = 0, ShowFiledContentValue = "" })
                    </div>
                </div>
               
        </abp-modal-body>
        <abp-modal-footer>
            <abp-button id="btnManifestCancel"
                        text="Cancel"
                        button-type="Default"
                        onclick="ManifestCancel()" />
            <abp-button id="btnManifestSave"
                        text="Ok"
                        button-type="Primary"
                        onclick="updateMawbParent()" />
        </abp-modal-footer>
    </abp-modal>
</form>