@section scripts
    {
    <abp-script src="/Pages/ReportScreen/VolumeProfitReport.js" />

    <script type="text/javascript">var l = abp.localization.getResource('Freight');

        $(document).ready(function () {
       
            // Awb Cancel Area
     
            $("#checkboxAwbCancelled").change(function () {
                if (this.checked) {
                    console.log("here");
                    $("#AwbCancelledDate").removeAttr("disabled");
                    $("#AwbCancelledOpId").removeAttr("disabled");
                    $("#ReasonForCancel").removeAttr("disabled");
                    $("#AwbCancelledDate").val(getDate());
                } else {
                    $("#AwbCancelledDate").attr("disabled", "disabled");
                    $("#AwbCancelledOpId").attr("disabled", "disabled");
                    $("#ReasonForCancel").attr("disabled", "disabled");
                    $("#AwbCancelledDate").val('');
                    $("#AwbCancelledOpId").val('');
                    $("#ReasonForCancel").val('');

                }
            });

            // Calculate Weight Kg to Lb
            $("#GrossWeightKg").on('keyup', function () {
                $("#GrossWeightLb").val(Math.round($("#GrossWeightKg").val() * 2.2046226218 * 100) / 100);
            });
            $("#AwbGrossWeightKg").on('keyup', function () {
                $("#AwbGrossWeightLb").val(Math.round($("#AwbGrossWeightKg").val() * 2.2046226218 * 100) / 100);
            });
            $("#ChargeableWeightKg").on('keyup', function () {
                $("#ChargeableWeightLb").val(Math.round($("#ChargeableWeightKg").val() * 2.2046226218 * 100) / 100);
            });
            $("#AwbChargeableWeightKg").on('keyup', function () {
                $("#AwbChargeableWeightLb").val(Math.round($("#AwbChargeableWeightKg").val() * 2.2046226218 * 100) / 100);
            });
            // Calculate Weight Lb to Kg
            $("#GrossWeightLb").on('keyup', function () {
                $("#GrossWeightKg").val(Math.round($("#GrossWeightLb").val() * 0.45359237 * 100) / 100);
            });
            $("#AwbGrossWeightLb").on('keyup', function () {
                $("#AwbGrossWeightKg").val(Math.round($("#AwbGrossWeightLb").val() * 0.45359237 * 100) / 100);
            });
            $("#ChargeableWeightLb").on('keyup', function () {
                $("#ChargeableWeightKg").val(Math.round($("#ChargeableWeightLb").val() * 0.45359237 * 100) / 100);
            });
            $("#AwbChargeableWeightLb").on('keyup', function () {
                $("#AwbChargeableWeightKg").val(Math.round($("#AwbChargeableWeightLb").val() * 0.45359237 * 100) / 100);
            });

            $("#VPRM_MinProfit").prop("disabled", true);
            $("#VPRM_MaxProfit").prop("disabled", true);

            $("input[name='VPRM.Report']").eq(0).prop('checked', 'checked');
            $("input[name='VPRM.OutputFormat']").eq(0).prop('checked', 'checked');
            $("input[name='VPRM.Profit']").eq(0).prop('checked', 'checked');

            var l = abp.localization.getResource('Freight');
            $('#tradePartnerDDL').select2({
                placeholder: l("Dropdown:Select"),
                allowClear: true
            });
            $('#tradePartnerDDL').closest('div').css('margin-bottom', '50px');
        });

        $('#createForm').on('abp-ajax-success', function () {
            event.preventDefault();
            abp.message.success('Successfully saved.');
        });
    
        function getDate() {
            var date = new Date();
            var dd = date.getDate();
            var mm = date.getMonth() + 1;
            var yyyy = date.getFullYear();
            var todayDate = mm + "-" + dd + "-" + yyyy;
            return todayDate;
        }

        function showTable() {
            
            var outputFormat = $("input[name='VPRM.OutputFormat']:checked").val();

            if (outputFormat === "PDF") {
                var shippingType = '';
                var reportType = '';
                var salesType = '';
                 var outputType = '';

                if ($('#VPRM_IsOceanImport')[0].checked) shippingType += 'OceanImport,';
                if ($('#VPRM_IsOceanExport')[0].checked) shippingType += 'OceanExport,';
                if ($('#VPRM_IsAirExport')[0].checked) shippingType += 'AirExport,';
                if ($('#VPRM_IsAirImport')[0].checked) shippingType += 'AirImport,';
                if ($('#VPRM_IsTruck')[0].checked) shippingType += 'Truck,';
                if ($('#VPRM_IsMisc')[0].checked) shippingType += 'Misc,';
                if ($('#VPRM_IsWarehouse')[0].checked) shippingType += 'Warehouse';
                shippingType = shippingType.replace(/^,|,$/g, '');

                if ($("input[name='VPRM.Report']:checked").val() == 'vp') reportType = 'Volume & Profit';
                if ($("input[name='VPRM.Report']:checked").val() == 'v') reportType = 'Volume Only';

                if ($('#VPRM_IsNA')[0].checked) salesType += 'N/A,';
                if ($('#VPRM_IsCOLoad')[0].checked) salesType += 'CO-LOAD,';
                if ($('#VPRM_IsFreeCargo')[0].checked) salesType += 'FREE CARGO,';
                if ($('#VPRM_IsNomi')[0].checked) salesType += 'NOMI';
                salesType = salesType.replace(/^,|,$/g, '');
                outputType = $('#VPRM_OutputType').val();
                OpenWindow('/Docs/TotalProfitVolumeSummary?shippingType=' + shippingType + '&reportType=' + reportType + '&salesType=' + salesType+'&outputType='+outputType);
            }
            else {
                var isDataTable = $.fn.dataTable.isDataTable('#mawReport');

                var createForm = $('#createForm');
                var form = createForm.serializeFormToObject();
                //Update freightCode for model
                if (form != null && form.vPRM != null && form.vPRM.freightCode != null)
                {
                    form.vPRM.freightCode = $('#VPRM_FreightCode').val();
                }
                if (!isDataTable) {
                    dolphin.freight.reportLog.reportLog.getMawbReport(form.vPRM).done(function (res) {
                        console.log(res);
                        LoadDataTable(res);
                    })

                    $('#result').show();
                }
                else {
                    var table = $('#mawReport').DataTable();
                    dolphin.freight.reportLog.reportLog.getMawbReport(form.vPRM).done(function (res) {
                        console.log(res);
                        table.destroy();
                        LoadDataTable(res);
                    });
                }
            }
        }

        function renderNegativeInRed(data, type, row, meta) {
            if (data != null) {
                let formattedData = parseFloat(data).toFixed(2);
                if (parseFloat(data) < 0) {
                    return '<span style="color: red;">' + formattedData!=null?formattedData:0 + '</span>';
                } else {
                    return formattedData;
                }
            }
            return '0';
        }

