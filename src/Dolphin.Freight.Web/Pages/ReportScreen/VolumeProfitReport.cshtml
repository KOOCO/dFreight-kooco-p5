        $("input[name='VPRM.Profit']").on("change", function () {
            var selectedValue = $("input[name='VPRM.Profit']:checked").val();

            if (selectedValue === "range") {
                $("#VPRM_MinProfit").prop("disabled", false);
                $("#VPRM_MaxProfit").prop("disabled", false);
            } else {
                $("#VPRM_MinProfit").prop("disabled", true);
                $("#VPRM_MaxProfit").prop("disabled", true);
            }
        });

        $("input[name='VPRM.OutputFormat']").on("change", function () {
            var selectedValue = $("input[name='VPRM.OutputFormat']:checked").val();

            if (selectedValue === "DS") {
                $('#VPRM_IsVolumeDetail, #VPRM_IsProfitDetail, #VPRM_IsContainerDetail').prop('disabled', false);
                $("#OutputBySection").show();
                $("#result").show();
            } 
            else {
                $('#VPRM_IsVolumeDetail, #VPRM_IsProfitDetail, #VPRM_IsContainerDetail').prop('disabled', true);
                $("#OutputBySection").hide();
                $("#result").hide();
            }
        });

        $("input[name='VPRM.Report']").on("change", function (){
            var selectedValue = $("input[name='VPRM.Report']:checked").val();

            if (selectedValue === "vp") {
                $("input[name='VPRM.Profit']").prop('disabled', false);
                $("#VPRM_IsProfitDetail").prop('disabled', false);
            }
            else {
                $("input[name='VPRM.Profit']").prop('disabled', true);
                $("#VPRM_IsProfitDetail").prop('disabled', true);
            }
        });

        $("#VPRM_IsOceanImport, #VPRM_IsOceanExport, #VPRM_IsAirImport, #VPRM_IsAirExport, #VPRM_IsNA, #VPRM_IsCOLoad, #VPRM_IsFreeCargo, #VPRM_IsNomi").on('click', function () {
            debugger;
            $("#showResultBtn").prop('disabled', false);

            if (!($("#VPRM_IsOceanImport").prop('checked') || $("#VPRM_IsOceanExport").prop('checked') || $("#VPRM_IsAirImport").prop('checked') || $("#VPRM_IsAirExport").prop('checked'))) {
                $("#showResultBtn").prop('disabled', true);
            }

            if (!($("#VPRM_IsNA").prop('checked') || $("#VPRM_IsCOLoad").prop('checked') || $("#VPRM_IsFreeCargo").prop('checked') || $("#VPRM_IsNomi").prop('checked'))) {
                $("#showResultBtn").prop('disabled', true);
            }
        });

        $('#VPRM_OutputType').change(function () {
            debugger;
            var selectedVal = $('#VPRM_OutputType').val();

            if (selectedVal == 'Shipper' || selectedVal == 'OverseaAgent' || selectedVal == 'Consignee' || selectedVal == 'Customer' || selectedVal == 'Carrier' || selectedVal == 'CustomsBroker' || selectedVal == 'Trucker' || selectedVal == 'AccountGroup' || selectedVal == 'BillTo' || selectedVal == 'ReferredBy') {

                $('#tradePartnerDDL').closest('div').show();
            }else{
                $('#tradePartnerDDL').closest('div').hide();
            }
        });

    </script>
}
<form abp-model="MawbModel" submit-button="true" data-ajaxForm="true" id="createForm">
    <abp-row>
        <abp-column size-md="_10">
            <abp-card border="Dark">
                <!-- 標題區域 -->
                <abp-card-header style="background:#555555;color:#FFFFFF;">
                    <abp-row>
                        <abp-column size-md="_12">
                            <abp-card-title>
                                @L["Title:VolumeProfileReport"]

                            </abp-card-title>
                        </abp-column>
                      @*   <abp-column size-md="_3" class="text-end">
                            <button type="button" class="btn btn-tool" data-bs-toggle="collapse" data-bs-target="#mawbDiv" aria-expanded="false" aria-controls="mawbDiv">
                                <i class="fa fa-caret-down" style="color:#FFFFFF;"></i>
                            </button>
                        </abp-column> *@
                    </abp-row>
                </abp-card-header>
                <!-- Mawb資訊欄 -->
                <abp-card-body id="mawbDiv">
                    <!--第一列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            <label asp-for="VPRM.Report" class="form-label">@L["Report"]</label>
                            <abp-radio asp-for="VPRM.Report" asp-items="Model.ReportList" inline="true" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <label asp-for="VPRM.OutputFormat" class="form-label">@L["OutputFormat"]</label>
                            <abp-radio asp-for="VPRM.OutputFormat" asp-items="Model.Format" inline="true" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <label asp-for="VPRM.PeriodType" class="form-label">@L["PeriodType"]</label>
                            <abp-radio asp-for="VPRM.PeriodType" asp-items="Model.PeriodType" inline="true" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <label asp-for="VPRM.PeriodRange" class="form-label">@L["PeriodRange"]</label>
                            <input asp-for="VPRM.PeriodRange" class="form-control" />
                        </abp-column>


                    </abp-row>
                    <!--第二列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            <abp-select asp-for="VPRM.Office" size="Small" asp-items="@Model.Office">
                            </abp-select>
                        </abp-column>
                        <abp-column size-md="_3">
                            @*<label asp-for="VPRM.ShippingType" class="form-label">Shipping Type</label>
                            <input asp-for="VPRM.ShippingType" class="form-control" />*@

                            <label asp-for="VPRM.ShippingType" class="form-label">@L["ShippingType"]</label>
                            <abp-row>
                                <abp-column size-md="Auto">
                                    <abp-input size-md="Auto" asp-for="VPRM.IsOceanImport" label="@L["OceanImport"]" checked/>
                                </abp-column>
                                <abp-column size-md="Auto">
                                    <abp-input size-md="Auto" checked asp-for="VPRM.IsOceanExport" label="@L["OceanExport"]" />
                                </abp-column>
                                <abp-column size-md="Auto">
                                    <abp-input size-md="Auto" checked asp-for="VPRM.IsAirImport" label="@L["AirImport"]" />
                                </abp-column>
                                <abp-column size-md="Auto">
                                    <abp-input size-md="Auto" checked asp-for="VPRM.IsAirExport" label="@L["AirExport"]" />
                                </abp-column>
                                <abp-column size-md="Auto">
                                    <abp-input size-md="Auto" asp-for="VPRM.IsTruck" label="@L["Truck"]" disabled/>
                                </abp-column>
                                <abp-column size-md="Auto">
                                    <abp-input size-md="Auto" asp-for="VPRM.IsMisc" label="@L["Misc."]" disabled/>
                                </abp-column>
                                <abp-column size-md="Auto">
                                    <abp-input size-md="Auto" asp-for="VPRM.IsWarehouse" label="@L["Warehouse"]" disabled />
                                </abp-column>
                            </abp-row>

                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-select asp-for="VPRM.Currency" size="Small" asp-items="@Model.Currency">
                            </abp-select>
                        </abp-column>
                        <abp-column size-md="_3">
                            <label asp-for="VPRM.Profit" class="form-label">@L["Profit"]</label>
                            <abp-radio asp-for="VPRM.Profit" asp-items="Model.Profit" inline="true" />
                            <div style="display:flex; padding-top:10px;">
                                <input asp-for="VPRM.MinProfit" class="form-control" style="margin-right:10px;"/>
                                <input asp-for="VPRM.MaxProfit" class="form-control" />
                            </div>
                        </abp-column>
