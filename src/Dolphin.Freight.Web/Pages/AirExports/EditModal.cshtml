                }
                else {

                    var ajaxParams = undefined;

                    Swal.fire({
                        title: 'Loading...',
                        didOpen: () => {
                            Swal.showLoading()
                        }
                    })

                    abp.ajax($.extend(true, {
                        url: abp.appPath + 'api/ImportExport/AirExportBasicHawb?Id=' + hawbId +'&hawbNo=' + hawbNo,
                        type: 'GET',
                        dataType: "html",
