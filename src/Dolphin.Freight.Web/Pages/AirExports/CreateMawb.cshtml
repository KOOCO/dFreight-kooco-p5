                                    <i class="fa fa-caret-down" style="color:#FFFFFF;"></i>
                            </button>
                        </abp-column>
                    </abp-row>
                </abp-card-header>
                <!-- Mawb資訊欄 -->
                <abp-card-body id="mawbDiv">
                    <!--第一列-->
                    <abp-row>
                        <abp-input asp-for="MawbModel.Id" type="hidden" />
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.FilingNo" disabled="true" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.MawbCarrierId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "MawbCarrierId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.MawbCarrierId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.IssuingCarrierId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "IssuingCarrierId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.IssuingCarrierId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-select asp-for="MawbModel.AwbType" size="Small"/>
                        </abp-column>
                    </abp-row>
                    <!--第二列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.MawbNo" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.AwbDate" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.ItnNo" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.ShipperId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "ShipperId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.ShipperId) })
                        </abp-column>
                    </abp-row>
                    <!--第三列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.ConsigneeId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "ConsigneeId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.ConsigneeId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.NotifyId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "NotifyId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.NotifyId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.PostDate" size="Small" type="text" abp-data-datepicker="false" readonly="true" />
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.OfficeId", AspItems = _dropdown.SubstationLookupList, IsShowLabel = true, FieldName = "OfficeId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.OfficeId) })
                            <span asp-validation-for="MawbModel.OfficeId" class="text-danger"></span>
                        </abp-column>
                    </abp-row>
                    <!--第四列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.IsDirectMaster"/>
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.AwbAcctCarrierId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "AwbAcctCarrierId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.AwbAcctCarrierId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.CoLoaderId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "CoLoaderId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.CoLoaderId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.MawbOperatorId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "MawbOperatorId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.MawbOperatorId) })
                        </abp-column>
                    </abp-row>
                    <!--第五列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.DepatureId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "DepatureId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.DepatureId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            <label asp-for="MawbModel.DepatureDate" class="redStar form-label">Depature Date</label>
                            <span asp-validation-for="MawbModel.DepatureDate" class="text-danger"></span>
                            <input asp-for="MawbModel.DepatureDate" class="form-control" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.FlightNo" size="Small"/>
                        </abp-column>
                        <abp-column size-md="_3">
                            <a class="btn btn-primary btn-sm" data-bs-toggle="collapse" href="#collapseRoute" role="button" aria-expanded="false" aria-controls="collapseRoute">
                                <i class="fa fa-plane" aria-hidden="true"></i>
                                Add Connecting Flight
                            </a>
                        </abp-column>
                    </abp-row>
                    <!--第六列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.DestinationId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "DestinationId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.DestinationId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.DVCarriage" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.DVCustomer" size="Small" />
                        </abp-column>
                    </abp-row>
                    <!--第七列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.Insurance" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.CarrierSpotNo" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-row>
                                <abp-column size-md="_3">
                                    Wtval
                                </abp-column>
                                <abp-column>
                                    <abp-radio asp-for="MawbModel.WtVal" inline="true" />
                                </abp-column>
                            </abp-row>
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-row>
                                <abp-column size-md="_3">
                                    Other
                                </abp-column>
                                <abp-column>
                                    <abp-radio asp-for="MawbModel.Other" inline="true" />
                                </abp-column>
                            </abp-row>
                        </abp-column>
                    </abp-row>
                    <!--第八列-->
                    <abp-row>
                        <abp-column size-md="_3">
                            @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.DeliveryId", AspItems = _dropdown.TradePartnerLookupList, IsShowLabel = true, FieldName = "DeliveryId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.DeliveryId) })
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.ShippingInfo" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.ShipperLoad" size="Small" />
                        </abp-column>
                        <abp-column size-md="_3">
                            <abp-input asp-for="MawbModel.Sci" size="Small" />
                        </abp-column>
                    </abp-row>
                    <!--Route區塊-->
                    <abp-row>
                        <div class="collapse" id="collapseRoute"> 
                            <div class="card card-body table-responsive">
                                <h3>Route</h3>
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>Airport</th>
                                            <th>Arrival Date/Time</th>
                                            <th>Depature Date/Time</th>
                                            <th>Flight No.</th>
                                            <th>Carrier</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <th>Trans1</th>
                                            <td>
                                                @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.RouteTrans1Id", AspItems = _dropdown.AirportLookupList, IsShowLabel = false, FieldName = "RouteTrans1Id", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.RouteTrans1Id) })
                                            </td>
                                            <td>
                                               <input asp-for="MawbModel.RouteTrans1ArrivalDate" size="Small" />
                                            </td>
                                            <td>
                                                <input asp-for="MawbModel.RouteTrans1DepatureDate" size="Small" />
                                            </td>
                                            <td>
                                                <input asp-for="MawbModel.RouteTrans1FlightNo" size="Small" />
                                            </td>
                                            <td>
                                                @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.RouteTrans1CarrierId", AspItems = _dropdown.AirportLookupList, IsShowLabel = false, FieldName = "RouteTrans1CarrierId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.RouteTrans1CarrierId) })
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>Trans2</th>
                                            <td>
                                                @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.RouteTrans2Id", AspItems = _dropdown.AirportLookupList, IsShowLabel = false, FieldName = "RouteTrans2Id", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.RouteTrans2Id) })
                                            </td>
                                            <td>
                                                <input asp-for="MawbModel.RouteTrans2ArrivalDate" size="Small" />
                                            </td>
                                            <td>
                                                <input asp-for="MawbModel.RouteTrans2DepatureDate" size="Small" />
                                            </td>
                                            <td>
                                                <input asp-for="MawbModel.RouteTrans2FlightNo" size="Small" />
                                            </td>
                                            <td>
                                                @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.RouteTrans2CarrierId", AspItems = _dropdown.AirportLookupList, IsShowLabel = false, FieldName = "RouteTrans2CarrierId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.RouteTrans2CarrierId) })
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>Trans3</th>
                                            <td>
                                                @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.RouteTrans3Id", AspItems = _dropdown.AirportLookupList, IsShowLabel = false, FieldName = "RouteTrans3Id", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.RouteTrans3Id) })
                                            </td>
                                            <td>
                                               <input asp-for="MawbModel.RouteTrans3ArrivalDate" size="Small" />
                                            </td>
                                            <td>
                                                <input asp-for="MawbModel.RouteTrans3DepatureDate" size="Small" />
                                            </td>
                                            <td>
                                                <input asp-for="MawbModel.RouteTrans3FlightNo" size="Small" />
                                            </td>
                                            <td>
                                                @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.RouteTrans3CarrierId", AspItems = _dropdown.AirportLookupList, IsShowLabel = false, FieldName = "RouteTrans3CarrierId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.RouteTrans3CarrierId) })
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </abp-row>
                    <!--第九列 Package, Weight Settings Area-->
                    <abp-row>
                        <div class="container">
                            <div class="row">
                                <div class="col">
                                    <table class="table table-borderless">
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                                <th></th>
                                            </tr>
                                         </thead>
                                         <tbody>
                                            <tr>
                                                <th>Package</th>
                                                <td><input asp-for="MawbModel.Package" class="form-control" /></td>
                                                <td colspan="3">
                                                    @await Component.InvokeAsync(typeof(DropdownComponent), new ComponentData { Name = "MawbModel.PackageUnitLookupList", AspItems = _dropdown.PackageUnitLookupList, IsShowLabel = false, FieldName = "MawbPackageUnitId", SelectType = 0, ShowFiledContentValue = Convert.ToString(Model.MawbModel.MawbPackageUnitId) })
