@page
@using Dolphin.Freight.Localization
@using Dolphin.Freight.Permissions
@using Dolphin.Freight.Web.Pages.AwbNoRanges
@using Microsoft.AspNetCore.Authorization
@using Microsoft.Extensions.Localization
@model Dolphin.Freight.Web.Pages.AirExports.HawbListModel
@inject IStringLocalizer<FreightResource> L
@inject IAuthorizationService AuthorizationService
@section scripts{
    <abp-script src="/Pages/AirExports/HawbList.js" />
}
<div class="page-header">
    <div class="breadcrumb-wrapper">
        <abp-breadcrumb>
            @*<abp-breadcrumb-item title=@L["Menu:AirExports"] />*@
            <abp-breadcrumb-item title=@L["Menu:AirExport"] />
            @*<abp-breadcrumb-item title=@L["Menu:AirExports.HawbList"] href="#" />*@
            <abp-breadcrumb-item title=@L["Menu:AirExport.HawbList"] href="#" />
        </abp-breadcrumb>
    </div>
</div>
<div class="page-content-area">
    <abp-card>
        <abp-card-header>
            <div class="title">
                @*@L["Menu:AirExports.HawbList"]*@
                @L["Menu:AirExport.HawbList"]
            </div>
        </abp-card-header>
        <abp-card-body>
            <div class="row col-12" style="text-align: center; margin:0!important">
                <abp-column size="_9"></abp-column>
                <abp-column size="_3" class="filterDiv">
                    <abp-input asp-for="Search" class="ml-4" />
                </abp-column>
            </div>
            <abp-button type="button" id="btnAirExportHawbListProfit" onclick="getHwabProfitReport()" class="btn btn-outline-secondary" disabled="true">
                <span uib-tooltip="">
                    <i class="fa fa-file-text-o"></i><font style="vertical-align: inherit;">
                        <font style="">
                            profit report
                        </font>
                    </font>
                </span>
            </abp-button>
            <abp-table class="nowrap" striped-rows="true" id="HawbListTable"></abp-table>
        </abp-card-body>
    </abp-card>
</div>

<style>
    .filterDiv div {
        display: flex;
        padding: 0 !important;
    }
</style>



