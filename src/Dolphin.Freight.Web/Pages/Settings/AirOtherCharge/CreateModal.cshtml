@page
@using Dolphin.Freight.Localization
@using Dolphin.Freight.Web.Pages.AirOtherCharge
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<FreightResource> L
@{
    Layout = null;
}

<abp-dynamic-form abp-model="AirOtherCharge" asp-page="/Settings/AirOtherCharge/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["Create"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-select asp-for="@Model.AirOtherCharge.companyType" asp-items="@Model.CompanyType" label="@L["companyType"].Value" display-required-symbol="true" />
            <abp-select asp-for="@Model.AirOtherCharge.paymentType" asp-items="@Model.PaymentList" label="@L["paymentType"].Value" display-required-symbol />
            <abp-input asp-for="@Model.AirOtherCharge.showOnMAWB" label="MAWB" />
            <abp-input asp-for="@Model.AirOtherCharge.showOnHAWB" label="HAWB" />
            <abp-select asp-for="@Model.AirOtherCharge.chargeItem" asp-items="@Model.ChargeItemList" label="@L["chargeItem"].Value" display-required-symbol />
            <abp-select asp-for="@Model.AirOtherCharge.chargeItemSubitem" asp-items="@Model.DescriptionList" label="@L["chargeItemSubitem"].Value" display-required-symbol />
            <abp-input asp-for="@Model.AirOtherCharge.chargeRate" label="@L["chargeRate"].Value" />
            <abp-select asp-for="@Model.AirOtherCharge.chargeRateUnit" asp-items="@Model.ChargeUnitList" label="@L["chargeRateUnit"].Value" display-required-symbol />
            <abp-input asp-for="@Model.AirOtherCharge.minPrice" label="@L["minPrice"].Value" />




        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
