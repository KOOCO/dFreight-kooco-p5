@using Volo.Abp.AspNetCore.Mvc.UI.Theme.Shared.Toolbars
@model Toolbar
@foreach (var toolbarItem in Model.Items.OrderBy(i => i.Order))
{
    if (toolbarItem.ComponentType.Name == "LanguageSwitchViewComponent")
    {
        <li class="dropdown dropdown-language nav-item"> @(await Component.InvokeAsync(toolbarItem.ComponentType))</li>
    }
    else
    {
        <li class="dropdown dropdown-user-info nav-item"> @(await Component.InvokeAsync(toolbarItem.ComponentType))</li>
    }
}